{
  "name": "raven",
  "version": "2.3.0",
  "private": true,
  "scripts": {
    "build": "ng build --configuration=prod --base-href /mpsserver/raven/ --deploy-url /mpsserver/raven/",
    "format": "npx prettier --write \"src/**/*.{js,ts,css,html}\"",
    "lint": "ng lint",
    "ng": "ng",
    "prebuild": "npm run version",
    "prestart": "npm run version",

{
  "name": "command-dictionary-normalizer",
  "version": "1.0.0",
  "description": "Normalizes command dictionaries",
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.jpl.nasa.gov/MPS/aerie"
  },
  "scripts": {
    "start": "node dist/index.js",
    "compile": "ts-node build.ts && tsc",
    "dev": "nodemon src/index.ts | pino-pretty",
    "dev:debug": "nodemon --exec 'node -r ts-node/register --inspect-brk' src/index.ts | pino-pretty",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "test": "mocha test/**/*.ts --exit",
    "test:debug": "mocha --inspect-brk test/**/*.ts --exit"
  },
  "author": "JPL",
  "license": "UNLICENSED",
  "dependencies": {
    "bluebird": "^3.5.3",
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-openapi-validator": "^1.0.0",
    "express-xml-bodyparser": "^0.3.0",
    "pino": "^5.11.1",
    "swagger-express-middleware": "^2.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.26",
    "@types/chai": "^4.1.7",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.16.1",
    "@types/express-xml-bodyparser": "^0.3.1",
    "@types/mocha": "^5.2.6",
    "@types/pino": "^5.8.5",
    "@types/shelljs": "^0.8.5",
    "@types/supertest": "^2.0.7",
    "@types/swagger-express-middleware": "^1.0.9",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "pino-pretty": "^2.6.1",
    "shelljs": "^0.8.3",
    "supertest": "^3.4.2",
    "ts-node": "^8.0.2",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  }

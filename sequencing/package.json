{
  "name": "sequencing",
  "version": "1.0.0",
  "description": "Service for managing sequences.",
  "main": "src/main.ts",
  "repository": {
    "type": "git",
    "url": "https://github.jpl.nasa.gov/MPS/aerie"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "start:dev": "npm run build && NODE_ENV=dev node dist/main.js",
    "start:local": "npm run build && NODE_ENV=local node dist/main.js",
    "start:prod": "npm run build && NODE_ENV=prod node dist/main.js",
    "test": "nyc npm run test:unit",
    "test:unit": "NODE_ENV=test mocha -r ts-node/register ./test/**/*.ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch:ts\" \"npm run watch:node\"",
    "watch:node": "nodemon dist/main.js",
    "watch:ts": "tsc -w"
  },
  "author": "JPL",
  "license": "UNLICENSED",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "decoders": "^1.13.1",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.16.4",
    "ip": "^1.1.5",
    "lodash": "^4.17.11",
    "mongodb": "^3.2.4",
    "morgan": "^1.9.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.1",
    "@types/ip": "^1.1.0",
    "@types/lodash": "^4.14.129",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.27",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.2",
    "@types/sinon": "^7.0.11",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
    "concurrently": "^4.1.0",
    "mocha": "^6.1.4",
    "mongodb-memory-server": "^5.1.2",
    "nodemon": "^1.19.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.3",
    "sinon": "^7.3.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.5"
  }
}

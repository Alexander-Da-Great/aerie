{
  "name": "nest",
  "version": "2.2.0",
  "private": true,
  "scripts": {
    "analyze": "npx webpack-bundle-analyzer ./dist/stats.json",
    "build-prod": "ng build --configuration=prod",
    "build:stats": "ng build --configuration=prod --stats-json",
    "build-prod-mpsserver": "ng build --outputPath=dist-mpsserver --configuration=prod-mpsserver --base-href /mpsserver/raven/ --deploy-url /mpsserver/raven/",
    "docs": "compodoc -p src/tsconfig.app.json -s",
    "e2e": "protractor e2e/protractor.conf.js",
    "e2e:build-and-run": "ng e2e",
    "e2e:upload": "python scripts/testrail/main.py",
    "format:check": "npx prettier --check \"src/**/*.{js,ts,css,html}\"",
    "format:write": "npx prettier --write \"src/**/*.{js,ts,css,html}\"",
    "integration-test": "ng serve --disable-host-check & bash ../robot/run.sh",
    "license:check": "/bin/bash ../scripts/license-headers -d -l ../scripts/license-template.txt -e js -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -d -l ../scripts/license-template.txt -e ts -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -d -l ../scripts/license-template.txt -e css -s \"Copyright\" ./src/app/",
    "license:write": "/bin/bash ../scripts/license-headers -l ../scripts/license-template.txt -e js -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -l ../scripts/license-template.txt -e ts -s \"Copyright\" ./src/app/ && /bin/bash ../scripts/license-headers -l ../scripts/license-template.txt -e css -s \"Copyright\" ./src/app/",
    "lint": "ng lint",
    "lint-prod": "ng lint --tslintConfig=./src/tslint.prod.json",
    "ng": "ng",
    "postinstall": "npx webdriver-manager update",
    "prebuild-prod": "npm run version",
    "prebuild-prod-mpsserver": "npm run version",
    "prestart": "npm run version",
    "pretest-for-build": "npm run version",
    "start": "ng serve",
    "start-dev": "bash ../scripts/post-adaptation.sh && ng serve",
    "test": "ng test --code-coverage --watch=false",
    "test:watch": "ng test",
    "test-for-build": "ng test --code-coverage --watch=false --browsers FirefoxHeadless",
    "version": "ts-node git.version.ts"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run format:write"
    }
  },
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/flex-layout": "8.0.0-beta.26",
    "@angular/forms": "^8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@ngrx/effects": "^8.0.0",
    "@ngrx/router-store": "^8.0.0",
    "@ngrx/store": "^8.0.0",
    "@ngrx/store-devtools": "^8.0.0",
    "ag-grid-angular": "^21.0.1",
    "ag-grid-community": "^21.0.1",
    "angular-sortablejs": "^2.7.0",
    "angular-split": "^3.0.1",
    "codemirror": "^5.47.0",
    "d3": "^5.9.2",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "lodash-es": "^4.17.11",
    "material-design-icons": "^3.0.1",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.25",
    "ng-pick-datetime": "^7.0.0",
    "ngx-color": "^3.0.3",
    "ngx-toastr": "^10.0.4",
    "rxjs": "~6.5.2",
    "sortablejs": "^1.9.0",
    "strip-json-comments": "^3.0.1",
    "tslib": "^1.9.3",
    "uuid": "^3.3.2",
    "web-animations-js": "github:angular/web-animations-js#release_pr208",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.2",
    "@angular/cli": "^8.0.2",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@compodoc/compodoc": "^1.1.9",
    "@ngrx/schematics": "^8.0.0",
    "@types/codemirror": "0.0.74",
    "@types/d3": "^5.7.2",
    "@types/file-saver": "^2.0.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/lodash-es": "^4.17.3",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "~8.9.4",
    "@types/strip-json-comments": "0.0.30",
    "@types/uuid": "^3.4.4",
    "codelyzer": "^5.1.0",
    "husky": "^2.4.0",
    "jasmine-core": "~3.4.0",
    "jasmine-marbles": "^0.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-firefox-launcher": "~1.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.2",
    "karma-junit-reporter": "~1.2.0",
    "prettier": "^1.18.2",
    "protractor": "^5.4.2",
    "sonar-scanner": "^3.1.0",
    "sonarjs": "^1.0.0",
    "ts-node": "~8.2.0",
    "tslint": "~5.17.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-jasmine": "^1.0.3",
    "typescript": "~3.4.5"
  }
}
